@model IEnumerable<prototipo1204.Models.Cliente>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.cpf)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.nomeCompleto)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dataNasc)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.senhaCliente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.emailCliente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.endereco)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.cpf)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.nomeCompleto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dataNasc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.senhaCliente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.emailCliente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.endereco.idEnd)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.idCliente">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.idCliente">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.idCliente">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
